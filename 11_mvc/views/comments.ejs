    <h2>댓글 목록</h2>
    <a href="/">홈으로 이동하기</a>
    <ul>
      <% for(let i=0; i<commentInfo.length; i++){ %>
        <li>
          <b><%= commentInfo[i].userid %></b>
          <a href="/comment/<%= i+1 %>"><%= commentInfo[i].comment%></a>
        </li>
        <% } %>
    </ul>
  </body>
</html>
